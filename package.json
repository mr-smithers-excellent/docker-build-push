{
  "name": "docker-build-push-action",
  "version": "5.7.0",
  "description": "Docker Build & Push Action",
  "main": "src/index.js",
  "scripts": {
    "build": "ncc build src/main.js",
    "precommit": "npm run build && git add dist/",
    "lint": "eslint 'src/**.js' 'tests/**.js' --fix",
    "test": "eslint 'src/**.js' 'tests/**.js' && jest --coverage",
    "test:watch": "npm run test -- --watchAll",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mr-smithers-excellent/docker-build-push-action.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "Docker",
    "GCR",
    "ECR",
    "Docker Hub"
  ],
  "author": "Sean Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mr-smithers-excellent/docker-build-push-action/issues"
  },
  "homepage": "https://github.com/mr-smithers-excellent/docker-build-push-action#readme",
  "dependencies": {
    "18": "^0.0.0",
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "mockdate": "^3.0.5",
    "prettier": "^3.1.1"
  }
}
